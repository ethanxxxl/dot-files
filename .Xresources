! Fonts
URxvt*font: xft:Terminess\ Powerline:size=12
URxvt.letterSpace: 0


URxvt.scrollBar: false

! Perl Extensions
URxvt.perl-ext-common: default,clipboard

! Copy-Paste Keys
URxvt.iso14755: false
URxvt.iso14655_52: false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysim.Shift-Control-V: perl:clipboard:paste

! Colors
*background:   [97]#282a36
*foreground:   #eff0eb
*cursorColor: #9aedfe

*color0:      #33303b
*color8:      #4f4b58
*color1:      #ff5c57
*color9:      #ff5c57
*color2:      #5af78e
*color10:     #5af78e
